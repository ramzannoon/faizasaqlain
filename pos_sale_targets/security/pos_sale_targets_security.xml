<?xml version="1.0" encoding="utf-8"?>
<odoo>
	
     <record model="ir.module.category" id="module_pos_reports">
        <field name="name">POS Reports</field>
        <field name="description">Helps you handle your POS Reports needs</field>
        <field name="sequence">15</field>
     </record>

	 <record id="group_pos_reports_user" model="res.groups">
        <field name="name">Account group</field>
        <field name="category_id" ref="pos_sale_targets.module_pos_reports"/>
         <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_pos_reports_coordinator" model="res.groups">
        <field name="name">Account group Coordinator</field>
<!--        <field name="category_id" ref="pos_sale_targets"/>-->
        <field name="implied_ids" eval="[(4, ref('pos_sale_targets.group_pos_reports_user'))]"/>
    </record>

    <record id="group_pos_reports_manager" model="res.groups">
        <field name="name">Account group Manager</field>
<!--        <field name="category_id" ref="pos_sale_targets"/>-->
        <field name="implied_ids" eval="[(4, ref('pos_sale_targets.group_pos_reports_coordinator'))]"/>
    </record>

</odoo>

